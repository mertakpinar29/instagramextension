{
    "name": "Get Unfollowers",
    "version": "1.1",
    "manifest_version": 2,
    "background": {
        "scripts": ["./background.js"]
    },
    "icons":{"128":"icon_128x128.png"},
    "options_page":"./options.html",
    "browser_action":{
        "default_popup": "popup.html",
        "default_icon":"icon_19x19.png"
    },
    "permissions":[
        "tabs",
        "https://www.instagram.com/*"
    ]
}